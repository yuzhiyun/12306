            android:name=".ticketList.control.TicketListActivity"
            android:label="@string/title_activity_ticket_list"
            android:parentActivityName=".main.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".personalInfor.InforActivity"
            android:label="@string/title_activity_infor"
            android:parentActivityName=".main.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ticketList.control.PayMoneyActivity"
            android:label="@string/title_activity_pay_moneyctivity"
            android:parentActivityName=".ticketList.control.TicketListActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:parentActivityName=".personalInfor.InforActivity"
            android:name=".personalInfor.EditInforActivity"
            android:label="@string/title_activity_edit_infor"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>